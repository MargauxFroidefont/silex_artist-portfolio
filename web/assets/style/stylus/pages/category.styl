// category styl

.category-container
    margin-top 31px
    overflow scroll
    white-space nowrap
    overflow-y hidden
    position relative
    z-index 1

    .category-img
        height 75vh
        display inline
        margin-left 10px

// category media queries 
@media (max-width:650px)

    .category-container
        margin-top 70px
